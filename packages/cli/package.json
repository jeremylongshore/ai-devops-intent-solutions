{
  "name": "@intentsolutions/blueprint",
  "version": "2.4.0",
  "description": "Enterprise AI documentation generator - CLI, MCP Server, and programmatic API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    }
  },
  "bin": {
    "blueprint": "./dist/cli.js",
    "blueprint-mcp": "./dist/mcp/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "start": "node dist/cli.js",
    "mcp": "node dist/mcp/index.js"
  },
  "keywords": [
    "cli",
    "mcp",
    "documentation",
    "prd-generator",
    "ai-documentation",
    "enterprise-templates",
    "claude",
    "cursor"
  ],
  "author": "Jeremy Longshore <jeremy@intentsolutions.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intent-solutions-io/intent-blueprint-docs.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@octokit/rest": "^20.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.0",
    "js-yaml": "^4.1.0",
    "marked": "^12.0.0",
    "ora": "^8.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
